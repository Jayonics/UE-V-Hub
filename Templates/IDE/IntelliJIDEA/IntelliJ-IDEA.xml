<?xml version="1.0" encoding="UTF-8"?>
<?xml-model href="SettingsLocationTemplate2013A.xsd" ?>
<!DOCTYPE SettingsLocationTemplate [
  <!ENTITY VERSION "IntelliJIDEA2020.3">
]>
<SettingsLocationTemplate xmlns="http://schemas.microsoft.com/UserExperienceVirtualization/2013A/SettingsLocationTemplate">
  <Name>IntelliJ IDEA</Name>
  <ID>IntelliJ-IDEA</ID>
  <Version>11</Version>
  <Processes>
    <Process>
      <Filename>idea64.exe</Filename>
      <Architecture>Win64</Architecture>
    </Process>
    <Process>
      <Filename>idea.exe</Filename>
      <Architecture>Win32</Architecture>
    </Process>
  </Processes>
  <Settings>
    <Asynchronous>true</Asynchronous>
    <PreventOverlappingSynchronization>false</PreventOverlappingSynchronization>
    <!-- Main IDEA Configuration -->
    <File>
      <Root><EnvironmentVariable>APPDATA</EnvironmentVariable></Root>
      <Path>JetBrains\&VERSION;</Path>
      <FileMask>*.vmoptions</FileMask>
    </File>
    <File>
      <Root><EnvironmentVariable>APPDATA</EnvironmentVariable></Root>
      <Path>JetBrains\&VERSION;</Path>
      <FileMask>idea.key</FileMask>
    </File>
    <File><Root><EnvironmentVariable>APPDATA</EnvironmentVariable></Root>
      <Path Recursive="true">JetBrains\consentOptions</Path>
    </File>
    <!-- There are 3 categories: -->
      <!-- 1. For no built in syncing (the default). -->
      <!-- 2. For using the JBA Settings sync plugin -->
      <!-- 3. For using the SettingsRepository sync plugin -->
    <!-- Disable each category as appropriate based on your setup -->
    <!-- NOTE: that DeleteIfNotFound is used on categories 2 and 3 because they are already backed up via their sync methods.
      If a user uses all 3 of these methods interchangably, their settings repository will triple in size unnecesairily.
      Therefore it makes sense to only ensure the backup of settings the user is not syncing within the IDE. -->

    <!-- 1. Main settings directories (No JBA Settings Sync or Repository Sync)-->
    <File>
      <Root><EnvironmentVariable>APPDATA</EnvironmentVariable></Root>
      <Path>JetBrains\&VERSION;</Path>
      <FileMask>*.txt</FileMask>
    </File>
    <File>
      <Root><EnvironmentVariable>APPDATA</EnvironmentVariable></Root>
      <Path Recursive="true">JetBrains\&VERSION;\codestyles</Path>
    </File>
    <File>
      <Root><EnvironmentVariable>APPDATA</EnvironmentVariable></Root>
      <Path Recursive="true">JetBrains\&VERSION;\colors</Path>
    </File>
    <File>
      <Root><EnvironmentVariable>APPDATA</EnvironmentVariable></Root>
      <Path Recursive="true">JetBrains\&VERSION;\fileTemplates</Path>
    </File>
    <File>
      <Root><EnvironmentVariable>APPDATA</EnvironmentVariable></Root>
      <Path Recursive="true">JetBrains\&VERSION;\filetypes</Path>
    </File>
    <File>
      <Root><EnvironmentVariable>APPDATA</EnvironmentVariable></Root>
      <Path Recursive="true">JetBrains\&VERSION;\inspection</Path>
    </File>
    <File>
      <Root><EnvironmentVariable>APPDATA</EnvironmentVariable></Root>
      <Path Recursive="true">JetBrains\&VERSION;\keymaps</Path>
    </File>
    <File>
      <Root><EnvironmentVariable>APPDATA</EnvironmentVariable></Root>
      <Path Recursive="true">JetBrains\&VERSION;\options</Path>
      <Exclude>
        <Path>JetBrains\&VERSION;\options</Path>
        <FileMask>recentProjects.xml</FileMask>
      </Exclude>
      <Exclude>
        <Path>JetBrains\&VERSION;\options</Path>
        <FileMask>window.state.xml</FileMask>
      </Exclude>
    </File>
    <File>
      <Root><EnvironmentVariable>APPDATA</EnvironmentVariable></Root>
      <Path Recursive="true">JetBrains\&VERSION;\scratches</Path>
    </File>
    <File>
      <Root><EnvironmentVariable>APPDATA</EnvironmentVariable></Root>
      <Path Recursive="true">JetBrains\&VERSION;\templates</Path>
    </File>
    <File>
      <Root><EnvironmentVariable>APPDATA</EnvironmentVariable></Root>
      <Path Recursive="true">JetBrains\&VERSION;\tools</Path>
    </File>
    <File>
      <Root><EnvironmentVariable>APPDATA</EnvironmentVariable></Root>
      <Path Recursive="true">JetBrains\&VERSION;\shelf</Path>
    </File>
    <!-- 2. Jetbrains account settings sync options -->
    <File>
      <Root><EnvironmentVariable>APPDATA</EnvironmentVariable></Root>
      <Path DeleteIfNotFound="true">JetBrains\&VERSION;\jba_config</Path>
      <FileMask>*.txt</FileMask>
    </File>
    <File>
      <Root><EnvironmentVariable>APPDATA</EnvironmentVariable></Root>
      <Path DeleteIfNotFound="true" Recursive="true">JetBrains\&VERSION;\jba_config\codestyles</Path>
    </File>
    <File>
      <Root><EnvironmentVariable>APPDATA</EnvironmentVariable></Root>
      <Path DeleteIfNotFound="true" Recursive="true">JetBrains\&VERSION;\jba_config\colors</Path>
    </File>
    <File>
      <Root><EnvironmentVariable>APPDATA</EnvironmentVariable></Root>
      <Path DeleteIfNotFound="true" Recursive="true">JetBrains\&VERSION;\jba_config\fileTemplates</Path>
    </File>
    <File>
      <Root><EnvironmentVariable>APPDATA</EnvironmentVariable></Root>
      <Path DeleteIfNotFound="true" Recursive="true">JetBrains\&VERSION;\jba_config\filetypes</Path>
    </File>
    <File>
      <Root><EnvironmentVariable>APPDATA</EnvironmentVariable></Root>
      <Path DeleteIfNotFound="true" Recursive="true">JetBrains\&VERSION;\jba_config\inspection</Path>
    </File>
    <File>
      <Root><EnvironmentVariable>APPDATA</EnvironmentVariable></Root>
      <Path DeleteIfNotFound="true" Recursive="true">JetBrains\&VERSION;\jba_config\keymaps</Path>
    </File>
    <File>
      <Root><EnvironmentVariable>APPDATA</EnvironmentVariable></Root>
      <Path DeleteIfNotFound="true" Recursive="true">JetBrains\&VERSION;\jba_config\options</Path>
      <Exclude>
        <Path DeleteIfNotFound="true">JetBrains\&VERSION;\jba_config\options</Path>
        <FileMask>recentProjects.xml</FileMask>
      </Exclude>
      <Exclude>
        <Path DeleteIfNotFound="true">JetBrains\&VERSION;\jba_config\options</Path>
        <FileMask>window.state.xml</FileMask>
      </Exclude>
    </File>
    <File>
      <Root><EnvironmentVariable>APPDATA</EnvironmentVariable></Root>
      <Path DeleteIfNotFound="true" Recursive="true">JetBrains\&VERSION;\jba_config\scratches</Path>
    </File>
    <File>
      <Root><EnvironmentVariable>APPDATA</EnvironmentVariable></Root>
      <Path DeleteIfNotFound="true" Recursive="true">JetBrains\&VERSION;\jba_config\templates</Path>
    </File>
    <File>
      <Root><EnvironmentVariable>APPDATA</EnvironmentVariable></Root>
      <Path DeleteIfNotFound="true" Recursive="true">JetBrains\&VERSION;\jba_config\tools</Path>
    </File>
    <File>
      <Root><EnvironmentVariable>APPDATA</EnvironmentVariable></Root>
      <Path DeleteIfNotFound="true" Recursive="true">JetBrains\&VERSION;\jba_config\shelf</Path>
    </File>
    <!-- 3. SettingsRepository Sync mode -->
    <File>
      <Root><EnvironmentVariable>APPDATA</EnvironmentVariable></Root>
      <Path DeleteIfNotFound="true">JetBrains\&VERSION;\settingsRepository</Path>
      <FileMask>*.txt</FileMask>
    </File>
    <File>
      <Root><EnvironmentVariable>APPDATA</EnvironmentVariable></Root>
      <Path DeleteIfNotFound="true" Recursive="true">JetBrains\&VERSION;\settingsRepository\codestyles</Path>
    </File>
    <File>
      <Root><EnvironmentVariable>APPDATA</EnvironmentVariable></Root>
      <Path DeleteIfNotFound="true" Recursive="true">JetBrains\&VERSION;\settingsRepository\colors</Path>
    </File>
    <File>
      <Root><EnvironmentVariable>APPDATA</EnvironmentVariable></Root>
      <Path DeleteIfNotFound="true" Recursive="true">JetBrains\&VERSION;\settingsRepository\fileTemplates</Path>
    </File>
    <File>
      <Root><EnvironmentVariable>APPDATA</EnvironmentVariable></Root>
      <Path DeleteIfNotFound="true" Recursive="true">JetBrains\&VERSION;\settingsRepository\filetypes</Path>
    </File>
    <File>
      <Root><EnvironmentVariable>APPDATA</EnvironmentVariable></Root>
      <Path DeleteIfNotFound="true" Recursive="true">JetBrains\&VERSION;\settingsRepository\inspection</Path>
    </File>
    <File>
      <Root><EnvironmentVariable>APPDATA</EnvironmentVariable></Root>
      <Path DeleteIfNotFound="true" Recursive="true">JetBrains\&VERSION;\settingsRepository\keymaps</Path>
    </File>
    <File>
      <Root><EnvironmentVariable>APPDATA</EnvironmentVariable></Root>
      <Path DeleteIfNotFound="true" Recursive="true">JetBrains\&VERSION;\settingsRepository\options</Path>
      <Exclude>
        <Path DeleteIfNotFound="true">JetBrains\&VERSION;\settingsRepository\options</Path>
        <FileMask>recentProjects.xml</FileMask>
      </Exclude>
      <Exclude>
        <Path DeleteIfNotFound="true">JetBrains\&VERSION;\settingsRepository\options</Path>
        <FileMask>window.state.xml</FileMask>
      </Exclude>
    </File>
    <File>
      <Root><EnvironmentVariable>APPDATA</EnvironmentVariable></Root>
      <Path DeleteIfNotFound="true" Recursive="true">JetBrains\&VERSION;\settingsRepository\scratches</Path>
    </File>
    <File>
      <Root><EnvironmentVariable>APPDATA</EnvironmentVariable></Root>
      <Path DeleteIfNotFound="true" Recursive="true">JetBrains\&VERSION;\settingsRepository\templates</Path>
    </File>
    <File>
      <Root><EnvironmentVariable>APPDATA</EnvironmentVariable></Root>
      <Path DeleteIfNotFound="true" Recursive="true">JetBrains\&VERSION;\settingsRepository\tools</Path>
    </File>
    <File>
      <Root><EnvironmentVariable>APPDATA</EnvironmentVariable></Root>
      <Path DeleteIfNotFound="true" Recursive="true">JetBrains\&VERSION;\settingsRepository\shelf</Path>
    </File>
  </Settings>
</SettingsLocationTemplate>